{
  "name": "demo",
  "version": "1.0.0",
  "description": "Demo-day starter",
  "author": "Write your name",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "deploy": "npm run generate && firebase deploy --only hosting --project nuxt-demo",
